		<dependency id="System.Reactive" version="3.0.0"/>
      </group>
      <group targetFramework="MonoAndroid1.0">
		<dependency id="System.Reactive" version="3.0.0"/>
		<dependency id="Plugin.CurrentActivity" version="1.0.1"/>
      </group>
    </dependencies>
  </metadata>
  <files>
		<file src="ReactiveBluetooth.Core\bin\Release\ReactiveBluetooth.*" target="lib\portable-net45+netcore45+wpa81\" exclude="**\*.pdb" />
		<file src="ReactiveBluetooth.iOS\bin\Release\ReactiveBluetooth.*" target="lib\Xamarin.iOS1.0\" exclude="**\*.pdb" />
		<file src="ReactiveBluetooth.Android\bin\Release\ReactiveBluetooth.*" target="lib\MonoAndroid1.0\" exclude="**\*.pdb" />
